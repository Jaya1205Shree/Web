<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="http://fonts.googleapis.com/css?family=Lato:300,400,400italic,600,700|Raleway:300,400,500,600,700|Crete+Round:400italic" rel="stylesheet" type="text/css" />
		<link rel="stylesheet" href="css/vendor/bootstrap/css/bootstrap.min.css">
		<link rel="stylesheet" href="css/vendor/fontawesome/css/font-awesome.min.css">
		<link rel="stylesheet" href="css/vendor/themify-icons/themify-icons.min.css">
		<link href="css/vendor/animate.css/animate.min.css" rel="stylesheet" media="screen">
		<link href="css/vendor/perfect-scrollbar/perfect-scrollbar.min.css" rel="stylesheet" media="screen">
		<link href="css/vendor/switchery/switchery.min.css" rel="stylesheet" media="screen">
		<link href="css/vendor/bootstrap-touchspin/jquery.bootstrap-touchspin.min.css" rel="stylesheet" media="screen">
		<link href="css/vendor/select2/select2.min.css" rel="stylesheet" media="screen">
		<link href="css/vendor/bootstrap-datepicker/bootstrap-datepicker3.standalone.min.css" rel="stylesheet" media="screen">
		<link href="css/vendor/bootstrap-timepicker/bootstrap-timepicker.min.css" rel="stylesheet" media="screen">
		<link rel="stylesheet" href="css/styles.css">
		<link rel="stylesheet" href="css/plugins.css">
		<link rel="stylesheet" href="css/themes/theme-1.css" id="skin_color" />
    <title>Document</title>
</head>

<body>
    <div id="app">
        <%-include('./partials/sidebar'); %>
            <div class="app-content">
                <%-include('./partials/header'); %>
            <div class="main-content">
                <div class="wrap-content container" id="container">
                    <!-- start: PAGE TITLE -->
                    <section id="page-title">
                        <div class="row">
                            <div class="col-sm-8">
                                <h1 class="mainTitle">Users | Medical History</h1>
                            </div>
                            <ol class="breadcrumb">
                                <li>
                                    <span>Users</span>
                                </li>
                                <li class="active">
                                    <span>Medical History</span>
                                </li>
                            </ol>
                        </div>
                    </section>
                    <div class="container-fluid container-fullw bg-white">
                        <div class="row">
                            <div class="col-md-12">
                                <h5 class="over-title margin-bottom-15">Users <span class="text-bold">Medical
                                <table border="1" class="table table-bordered">
                                    <tr align="center">
                                        <td colspan="4" style="font-size:20px;color:blue">
                                            Patient Details</td>
                                    </tr>

                                    <tr>
                                        <th scope>Patient Name</th>
                                        <td>
                                            <%= PatientName%>
                                        </td>
                                        <th scope>Patient Email</th>
                                        <td>
                                            <%= PatientEmail%>
                                        </td>
                                    </tr>
                                    <tr>
                                        <th scope>Patient Mobile Number</th>
                                        <td>
                                            <%= PatientContno%>
                                        </td>
                                        <th>Patient Address</th>
                                        <td>
                                            <%= PatientAdd%>
                                        </td>
                                    </tr>
                                    <tr>
                                        <th>Patient Gender</th>
                                        <td>
                                            <%= PatientGender%>
                                        </td>
                                        <th>Patient Age</th>
                                        <td>
                                            <%= PatientAge%>
                                        </td>
                                    </tr>
                                    <tr>

                                        <th>Patient Medical History(if any)</th>
                                        <td>
                                            <%= PatientMedhis%>
                                        </td>
                                        <th>Patient Reg Date</th>
                                        <td>
                                            <%= CreationDate%>
                                        </td>
                                    </tr>

                                </table>
                                <table id="datatable" class="table table-bordered dt-responsive nowrap"
                                    style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                                    <tr align="center">
                                        <th colspan="8">Medical History</th>
                                    </tr>
                                    <tr>
                                        <th>#</th>
                                        <th>Blood Pressure</th>
                                        <th>Weight</th>
                                        <th>Blood Sugar</th>
                                        <th>Body Temprature</th>
                                        <th>Medical Prescription</th>
                                        <th>Visit Date</th>
                                    </tr>
                                    <% message.forEach(function (appoint) { %>
                                        <tr>
                                            <td>
                                                <ul><li></li></ul>
                                            </td>
                                            <td>
                                                <%= appoint.BloodPressure%>
                                            </td>
                                            <td>
                                                <%= appoint.Weight%>
                                            </td>
                                            <td>
                                                <%= appoint.BloodSugar%>
                                            </td>
                                            <td>
                                                <%= appoint.Temperature%>
                                            </td>
                                            <td>
                                                <%= appoint.MedicalPres%>
                                            </td>
                                            <td>
                                                <%= appoint.CreationDate_1%>
                                            </td>
                                        </tr>
                                    <% }) %>
                                </table>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    </div>
    <%-include('./partials/footer'); %>
    </div>
    <script src="css/vendor/jquery/jquery.min.js"></script>
    <script src="css/vendor/bootstrap/js/bootstrap.min.js"></script>
    <script src="css/vendor/modernizr/modernizr.js"></script>
    <script src="css/vendor/jquery-cookie/jquery.cookie.js"></script>
    <script src="css/vendor/perfect-scrollbar/perfect-scrollbar.min.js"></script>
    <script src="css/vendor/switchery/switchery.min.js"></script>
    <script src="css/vendor/maskedinput/jquery.maskedinput.min.js"></script>
    <script src="css/vendor/bootstrap-touchspin/jquery.bootstrap-touchspin.min.js"></script>
    <script src="css/vendor/autosize/autosize.min.js"></script>
    <script src="css/vendor/selectFx/classie.js"></script>
    <script src="css/vendor/selectFx/selectFx.js"></script>
    <script src="css/vendor/select2/select2.min.js"></script>
    <script src="css/vendor/bootstrap-datepicker/bootstrap-datepicker.min.js"></script>
    <script src="css/vendor/bootstrap-timepicker/bootstrap-timepicker.min.js"></script>
    <script src="js/main.js"></script>
    <script src="js/form-elements.js"></script>
</body>

</html>
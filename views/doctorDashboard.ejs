<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link
        href="http://fonts.googleapis.com/css?family=Lato:300,400,400italic,600,700|Raleway:300,400,500,600,700|Crete+Round:400italic"
        rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="css/doctor/vendor/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="css/doctor/vendor/fontawesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="css/doctor/vendor/themify-icons/themify-icons.min.css">
    <link href="css/doctor/vendor/animate.css/animate.min.css" rel="stylesheet" media="screen">
    <link href="css/doctor/vendor/perfect-scrollbar/perfect-scrollbar.min.css" rel="stylesheet" media="screen">
    <link href="css/doctor/vendor/switchery/switchery.min.css" rel="stylesheet" media="screen">
    <link href="css/doctor/vendor/bootstrap-touchspin/jquery.bootstrap-touchspin.min.css" rel="stylesheet" media="screen">
    <link href="css/doctor/vendor/select2/select2.min.css" rel="stylesheet" media="screen">
    <link href="css/doctor/vendor/bootstrap-datepicker/bootstrap-datepicker3.standalone.min.css" rel="stylesheet" media="screen">
    <link href="css/doctor/vendor/bootstrap-timepicker/bootstrap-timepicker.min.css" rel="stylesheet" media="screen">
    <link rel="stylesheet" href="css/doctor/styles.css">
    <link rel="stylesheet" href="css/doctor/plugins.css">
    <link rel="stylesheet" href="css/doctor/themes/theme-1.css" id="skin_color" />


</head>

<body>
    <div id="app">
        <%-include('./partials/doctor/sidebar'); %>
            <div class="app-content">
                <%-include('./partials/doctor/header'); %>

                    <!-- end: TOP NAVBAR -->
                    <div class="main-content">
                        <div class="wrap-content container" id="container">
                            <!-- start: PAGE TITLE -->
                            <section id="page-title">
                                <div class="row">
                                    <div class="col-sm-8">
                                        <h1 class="mainTitle">Doctor | Dashboard</h1>
                                    </div>
                                    <ol class="breadcrumb">
                                        <li>
                                            <span>User</span>
                                        </li>
                                        <li class="active">
                                            <span>Dashboard</span>
                                        </li>
                                    </ol>
                                </div>
                            </section>
                            <!-- end: PAGE TITLE -->
                            <!-- start: BASIC EXAMPLE -->
                            <div class="container-fluid container-fullw bg-white">
                                <div class="row">
                                    <div class="col-sm-4">
                                        <div class="panel panel-white no-radius text-center">
                                            <div class="panel-body">
                                                <span class="fa-stack fa-2x"> <i
                                                        class="fa fa-square fa-stack-2x text-primary"></i> <i
                                                        class="fa fa-smile-o fa-stack-1x fa-inverse"></i> </span>
                                                <h2 class="StepTitle">My Profile</h2>

                                                <p class="links cl-effect-1">
                                                    <a href="/doctorEditProfile">
                                                        Update Profile
                                                    </a>
                                                </p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-4">
                                        <div class="panel panel-white no-radius text-center">
                                            <div class="panel-body">
                                                <span class="fa-stack fa-2x"> <i
                                                        class="fa fa-square fa-stack-2x text-primary"></i> <i
                                                        class="fa fa-paperclip fa-stack-1x fa-inverse"></i> </span>
                                                <h2 class="StepTitle">My Appointments</h2>

                                                <p class="cl-effect-1">
                                                    <a href="/doctorAppointmentHistory">
                                                        View Appointment History
                                                    </a>
                                                </p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-4">
                                        <div class="panel panel-white no-radius text-center">
                                            <div class="panel-body">
                                                <span class="fa-stack fa-2x"> <i class="fa fa-square fa-stack-2x text-primary"></i> <i class="fa fa-terminal fa-stack-1x fa-inverse"></i> </span>
                                                <h2 class="StepTitle"> Add Patient</h2>
                                                <p class="links cl-effect-1">
                                                    <a href="/doctorAddPatient">
                                                        Add Patient
                                                    </a>
                                                </p>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>

                        </div>
                    </div>
            </div>
            <%-include('./partials/footer'); %>
    </div>
    <!-- start: MAIN JAVASCRIPTS -->
    <script src="css/doctor/vendor/jquery/jquery.min.js"></script>
    <script src="css/doctor/vendor/bootstrap/js/bootstrap.min.js"></script>
    <script src="css/doctor/vendor/modernizr/modernizr.js"></script>
    <script src="css/doctor/vendor/jquery-cookie/jquery.cookie.js"></script>
    <script src="css/doctor/vendor/perfect-scrollbar/perfect-scrollbar.min.js"></script>
    <script src="css/doctor/vendor/switchery/switchery.min.js"></script>
    <script src="css/doctor/vendor/maskedinput/jquery.maskedinput.min.js"></script>
    <script src="css/doctor/vendor/bootstrap-touchspin/jquery.bootstrap-touchspin.min.js"></script>
    <script src="css/doctor/vendor/autosize/autosize.min.js"></script>
    <script src="css/doctor/vendor/selectFx/classie.js"></script>
    <script src="css/doctor/vendor/selectFx/selectFx.js"></script>
    <script src="css/doctor/vendor/select2/select2.min.js"></script>
    <script src="css/doctor/vendor/bootstrap-datepicker/bootstrap-datepicker.min.js"></script>
    <script src="css/doctor/vendor/bootstrap-timepicker/bootstrap-timepicker.min.js"></script>
    <script src="js/doctor/main.js"></script>
    <script src="js/doctor/form-elements.js"></script>
</body>

</html>